{
  "include": ["src/**/*.js", "src/**/*.vue"],
  "exclude": [
    "babel.*.js",
    "jest.*.js",
    "vue.*.js",
    "src/App.vue",
    "src/main.js",
    "src/components/example",
    "tests/**/*"
  ],
  "extension": [".js", ".vue"],
  "reporter": ["html", "lcov", "text"],
  "cache": false,
  "all": true,
  "check-coverage": false,
  "instrument": true,
  "sourceMap": true
}
